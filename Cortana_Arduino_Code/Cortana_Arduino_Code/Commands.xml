<?xml version="1.0" encoding="utf-8" ?> 
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us" Name="Commands">
    <CommandPrefix>
      Need Help
    </CommandPrefix>
      
    <Example>
      Massage please,
      Me move please
    </Example>
  
    <Command Name="Massage">
      <Example>
        I am in pain
      </Example>
      <ListenFor>
        I am in pain
      </ListenFor>
      <ListenFor>
        Massage please
      </ListenFor>
      <ListenFor>
        I'm in pain 
      </ListenFor>
      <ListenFor>
        I have pain in my thighs
      </ListenFor>
      <Feedback>
        Started your Massage
      </Feedback>
      <Navigate/>
    </Command>
  

    <Command Name="Move">
      <Example>
         moving please
      </Example>
      <ListenFor>
        moving please
      </ListenFor>
      <ListenFor>
        I can not move
      </ListenFor>
      <ListenFor>
        I can't move
      </ListenFor>
      <Feedback>
        Helping you move now
      </Feedback>
      <Navigate/>
    </Command>
  </CommandSet>
</VoiceCommands>